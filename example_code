instructor = request.forms.get('instructor')
	table = []
	with open(instructor, 'r') as input:
		fields = input.readline().strip().split(',')
		table = []
		for line in input:
			temp = line.strip().split(',')
			counter = 0
			temp_dict = {}
			for x in temp:
				temp_dict[fields[counter]] = x
				counter += 1
			table.append(temp_dict)
	return template('./web/result.html', data=table)
